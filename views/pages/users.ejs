<% include ../partials/header %>
<div class="jumbotron">
<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Email</th>
            <th>Password</th>
            <th><a  class="btn btn-outline-info" href="/newuser" role="button">ADD NEW</a></th>
        </tr>
    </thead>
    <tbody id="myTable">
    <%users.forEach(function(user){ %>
<tr>
            <td scope="row"><%= user.id %></td>
            <td><%= user.email %></td>
            <td><%= user.password %></td>
            <td><a  class="btn btn-outline-primary" href="/users/<%= user.id %>" role="button">EDIT</a></td>
        </tr>
    <% }); %>
        
        
    </tbody>
</table>
</div>